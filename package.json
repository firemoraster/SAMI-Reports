{
  "name": "sami-weekly-reports",
  "version": "1.0.0",
  "description": "Automated weekly reporting system with Telegram bot, Trello, and PDF integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:init": "ts-node src/database/init.ts",
    "db:seed": "ts-node src/database/seed.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "telegram-bot",
    "trello",
    "weekly-reports",
    "pdf",
    "automation"
  ],
  "author": "SAMI Team",
  "license": "MIT",
  "dependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "axios": "^1.6.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^3.2.0",
    "docx": "^9.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "i18next": "^23.7.16",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.15.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.8",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/pdfkit": "^0.17.5",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
